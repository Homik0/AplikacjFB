        <Lighting>
          <bumpInput>
            <Shadow />
          </bumpInput>
          <light>
            <javafx.scene.effect.Light.Distant color="#41cc00" />
          </light>
        </Lighting>
      </effect>
      <font>
        <Font name="System Bold" size="24.0" />
      </font>
    </Button>
    <Label alignment="CENTER" layoutX="15.0" layoutY="252.0" prefHeight="41.0" prefWidth="365.0" text="Szukany lokal" textAlignment="CENTER" textFill="BLACK">
